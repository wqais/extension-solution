{
    "manifest_version": 3,
    "name": "Simple Bookmarks",
    "version": "1.0",
    "description": "A minimalistic Chrome extension for bookmark management.",
    "permissions": ["storage", "activeTab"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "background": {
      "service_worker": "background.js"
    },
    "permissions": ["storage"]
  }

  
  
  